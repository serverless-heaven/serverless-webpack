{
  "name": "serverless-webpack",
  "version": "5.6.1",
  "description": "Serverless plugin to bundle your javascript with Webpack",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "Frank Schmid <fschmid740@googlemail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serverless-heaven/serverless-webpack.git"
  },
  "keywords": [
    "serverless",
    "1.0",
    "webpack",
    "babel",
    "typescript",
    "es6",
    "plugin",
    "runtime",
    "elm",
    "npm",
    "yarn"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/serverless-heaven/serverless-webpack/issues"
  },
  "homepage": "https://github.com/serverless-heaven/serverless-webpack#readme",
  "scripts": {
    "test": "mocha \"tests/**/*.test.js\" -R spec --recursive",
    "test-coverage": "nyc ./node_modules/mocha/bin/_mocha \"tests/**/*.test.js\" -R spec --recursive",
    "eslint": "eslint --ext .js *.js lib tests"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "tests/**/*.*",
      "coverage/**",
      "examples/**"
    ],
    "reporter": [
      "lcov",
      "clover",
      "text-summary"
    ],
    "report-dir": "./coverage"
  },
  "dependencies": {
    "archiver": "^5.3.0",
    "bluebird": "^3.7.2",
    "fs-extra": "^9.1.0",
    "glob": "^7.2.0",
    "is-builtin-module": "^3.1.0",
    "lodash": "^4.17.21",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/eslint-parser": "^7.17.0",
    "@serverless/test": "^9.0.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "mocha": "^8.4.0",
    "mockery": "^2.1.0",
    "nyc": "^15.1.0",
    "prettier": "^2.6.0",
    "serverless": "^3.8.0",
    "sinon": "^13.0.1",
    "sinon-chai": "^3.7.0",
    "unzipper": "^0.10.11"
  },
  "peerDependencies": {
    "serverless": "1 || 2 || 3",
    "webpack": ">= 3.0.0 < 6"
  },
  "optionalDependencies": {
    "ts-node": ">= 8.3.0"
  },
  "engines": {
    "node": ">= 10.12.0"
  },
  "mocha": {
    "require": [
      "@serverless/test/setup/patch",
      "@serverless/test/setup/log",
      "@serverless/test/setup/mock-homedir",
      "@serverless/test/setup/mock-cwd",
      "@serverless/test/setup/restore-env"
    ]
  }
}
